<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="700.0" prefWidth="500.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.qpims3.CustomerCreateFXMLController">
    <children>
      <AnchorPane prefHeight="686.0" prefWidth="460.0">
         <children>
              <Label text="Create Customer">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
              <Button fx:id="secondaryButton" layoutY="230.0" onAction="#Customer1switch" text="Cancel" />
            <Text layoutX="2.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="First name*" />
            <TextField fx:id="Fnametxt" layoutX="138.0" layoutY="40.0" prefHeight="26.0" prefWidth="191.0" />
            <Text layoutX="3.0" layoutY="93.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Last name*" />
            <TextField fx:id="Lnametxt" layoutX="138.0" layoutY="76.0" prefHeight="26.0" prefWidth="191.0" />
            <Text layoutX="5.0" layoutY="129.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Phone number*" />
            <TextField fx:id="Pnumbertxt" layoutX="138.0" layoutY="112.0" prefHeight="26.0" prefWidth="191.0" />
            <Text layoutX="5.0" layoutY="165.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Address" />
            <TextField fx:id="Addresstxt" layoutX="138.0" layoutY="148.0" prefHeight="26.0" prefWidth="191.0" />
            <Button fx:id="AddCustomer" layoutX="251.0" layoutY="230.0" onAction="#buttonAddCustomer" text="Add Customer" />
         </children>
      </AnchorPane>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
